<h1>Productos</h1>
<ul>
    {{#each payload}}
    <li>Titulo: {{title}}</li>
    <li>Description: {{description}}</li>
    <li>Price: {{price}}</li>
    <li>Code: {{code}}</li>
    <li>stock: {{stock}}</li>
    <li>category: {{category}}</li>
    <li>Thumbnails: {{thumbnails}}</li>
    {{/each}}
</ul>
<div>
    {{#if hasPrevPage}}
        <a href="{{prevLink}}"><button>← Back</button></a>
    {{/if}}

    {{#if hasNextPage}}
        <a href="{{nextLink}}"><button>Next →</button></a>
    {{/if}}
</div>
